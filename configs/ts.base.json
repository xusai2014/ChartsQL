{
    "compilerOptions": {
        "allowJs": true,
        "allowUmdGlobalAccess": true,
        "alwaysStrict": true,
        "assumeChangesOnlyAffectDirectDependencies": true,
        "checkJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "locale": "zh-CN",
        "module": "commonjs",
        "target": "ESNext",
        "strictNullChecks": true,
        "outDir":"../lib"
    }
}
